{
    "name": "confluxgroup/craft-relationshipeditor",
    "description": "Allows element relationships to be modified from the front-end without re-submitting all selections.",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "relationship editor"
    ],
    "support": {
        "docs": "https://github.com/ConfluxGroup/craft-relationshipeditor/blob/master/README.md",
        "issues": "https://github.com/ConfluxGroup/craft-relationshipeditor/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Conflux Group, Inc.",
            "homepage": "https://confluxgroup.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
          "confluxgroup\\relationshipeditor\\": "src/"
        }
    },
    "extra": {
        "name": "Relationship Editor",
        "handle": "relationshipeditor",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://github.com/ConfluxGroup/craft-relationshipeditor/blob/master/CHANGELOG.md",
        "class": "confluxgroup\\relationshipeditor\\RelationshipEditor"
    }
}
